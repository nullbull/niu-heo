<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.niu.user.server.mapper.AddressMapper">
    <select id="selectByUserId" resultType="com.github.niu.user.api.models.vo.AddressVO">
        select * from address where user_id = #{userId} and status != 0
    </select>
</mapper>
